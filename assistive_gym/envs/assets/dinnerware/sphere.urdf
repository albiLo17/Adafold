<?xml version="1.0" ?>
<robot name="sphere">

  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

  <link name="root">
    <inertial>
      <mass value="5.0"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.08"/>
        <!-- <box size="0.05 0.05 0.05"/> -->
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.08"/>
        <!-- <box size="0.05 0.05 0.05"/> -->
      </geometry>
    </collision>
  </link>
</robot>


